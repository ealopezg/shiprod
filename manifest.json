{
  "name": "SHIPROD",
  "version": "2.5.0",
  "manifest_version": 2,
  "description": "A painless way to SHIpping PROducts in Dolibarr for more info check https://github.com/ealopezg/shiprod",
  "icons": {
    "16": "icons/icon16.png",
    "20": "icons/icon20.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon20.png",
    "default_title": "Inventory",
    "default_popup": "src/page_action/page_action.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/dolibarr/*"],
      "all_frames": true,
      "js": [
        "src/inject/inject.js"
      ]
    }
  ],
  "permissions": ["http://*/dolibarr/*"]
}